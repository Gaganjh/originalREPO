<!-- 
  The test-<service>-config.xml file allows the developer to put in any test
  parameters that they would like to use for the unit tests. If the value isn't
  found in that file, the file below it is searched (default-<service-config.xml).
-->
<config>
  <!-- 
    Each node level is part of the 'dotted' notation in the key name.
    i.e. The 'ldap.context.factory' value is 'com.sun.jndi.ldap.LdapCtxFactory' from:
    <ldap>
    <context>
    <factory>com.sun.jndi.ldap.LdapCtxFactory</factory>
    </context>
    </ldap>
  -->
  <!-- Begin Withdrawal -->
  <withdrawal>
    <!-- Date Of Birth Limits -->
    <dateOfBirth>
      <minimumAge>15</minimumAge>
      <maximumAge>150</maximumAge>
    </dateOfBirth>
    <!-- Statuses -->
    <status>
      <draft>14</draft>
      <approved>W1</approved>
      <deleted>W2</deleted>
      <denied>W3</denied>
      <expired>W4</expired>
      <pending_approval>W5</pending_approval>
      <pending_review>W6</pending_review>
      <ready_for_entry>W7</ready_for_entry>
    </status>
  </withdrawal>
  <!-- End Withdrawal -->
</config>